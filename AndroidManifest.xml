<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.farmer">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission. ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".AllItems"
            android:parentActivityName=".Market_Price"
            android:label="Items"></activity>
        <activity
            android:name=".Market_Price"
            android:label="Market"
            android:parentActivityName=".MainActivity2" />
        <activity
            android:name=".AddItem"
            android:label="Add Commodity"
            android:parentActivityName=".MainActivity2" />
        <activity
            android:name=".Profile"
            android:label="Profile"
            android:parentActivityName=".MainActivity2" />
        <activity
            android:name=".Crop_Varities"
            android:label="Crop Varities"
            android:parentActivityName=".MainActivity2" />
        <activity
            android:name=".MarketPrice"
            android:label="Daily Price"
            android:parentActivityName=".MainActivity2" />
        <activity android:name=".MainActivity2" />
        <activity android:name=".VerifyPhoneActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>